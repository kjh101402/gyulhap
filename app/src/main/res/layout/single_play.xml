                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|left"
                    android:background="#341F1F"
                    android:gravity="top|left"
                    android:text=" 1 "
